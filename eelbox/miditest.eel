import "midi", "SDL";

export function main<args>
{
	OpenMIDI();
	for local i = 1, 10
	{
		local ev = ReadMIDI();
		if not ev
		{
			Delay(10);
			repeat;
		}
		print(EventName(ev.type), ": ");
		for local j = 1, sizeof ev - 1
			print(" ", key(ev, j), ":", index(ev, j));
		print("\n");
	}
	CloseMIDI();
	return 0;
}
